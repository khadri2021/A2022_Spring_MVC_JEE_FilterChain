<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>s1</servlet-name>
		<servlet-class>com.khadri.spring.jee.mvc.Servlet1</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>s1</servlet-name>
		<url-pattern>/serv1</url-pattern>
	</servlet-mapping>

	<filter>
		<filter-name>a</filter-name>
		<filter-class>com.khadri.spring.jee.mvc.AFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>a</filter-name>
		<url-pattern>/serv1</url-pattern>
	</filter-mapping>
	
	<filter>
		<filter-name>b</filter-name>
		<filter-class>com.khadri.spring.jee.mvc.BFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>b</filter-name>
		<url-pattern>/serv1</url-pattern>
	</filter-mapping>


	<filter>
		<filter-name>c</filter-name>
		<filter-class>com.khadri.spring.jee.mvc.CFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>c</filter-name>
		<url-pattern>/serv1</url-pattern>
	</filter-mapping>

</web-app>
